    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Edinyanga O. Ottoho",
            "email": "elzucky@gmail.com"
        },
        {
            "name": "apitoolkitio",
            "email": "hello@apitoolkit.io"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "APIToolkit": "src"
        }
    },
