{
  "name": "apitoolkit/apitoolkit-php",
  "description": "A PHP/Laravel SDK Wrapper for APIToolkit. It monitors incoming traffic, gathers the requests and sends the request to the apitoolkit servers.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Edinyanga O. Ottoho",
      "email": "elzucky@gmail.com"
    },
    {
      "name": "apitoolkitio",
      "email": "hello@apitoolkit.io"
    },
    {
      "name": "Michael Trojan Okoh",
      "email": "michael@okoh.co.uk"
    }
  ],
  "prefer-stable": false,
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "APIToolkit\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "APIToolkit\\APIToolkit\\Provider\\ApitoolkitServiceProvider"
      ]
    }
  },
  "require": {
    "google/cloud-pubsub": "^1.35",
    "illuminate/support": "^5.0|^6.0|^7.0|^8.0",
    "illuminate/http": "^5.0|^6.0|^7.0|^8.0",
    "ext-json": "*",
    "ext-curl": "*"
  }
}
